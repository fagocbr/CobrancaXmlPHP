{
  "name": "fagoc/cobrancaxml-php",
  "version": "0.1.5",
  "description": "",
  "type": "package",
  "license": "MIT",
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.6",
    "ext-soap": "*",
    "ext-pcre": "*"
  },
  "autoload": {
    "psr-4": {
      "CobrancaPHP\\": "source/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CobrancaPHP\\Test\\": "test/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5"
  },
  "scripts": {
    "test": "vendor/bin/phpunit --colors=always",
    "major": "./composer.release.sh major",
    "minor": "./composer.release.sh minor",
    "patch": "./composer.release.sh patch"
  },
  "config": {
    "process-timeout": 60000
  }
}
